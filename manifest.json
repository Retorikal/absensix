{
	"manifest_version": 2,

	"name": "absensix",
	"description": "Extension for SIX auto-attendance.",
	"version": "1.0",

	"browser_action": {
		//"default_popup": "popup.html",
		"default_icon": "favicon.png"
	},

	"content_scripts": [{
		"matches": [ "*://akademik.itb.ac.id/app/*/kelas/pertemuan/jadwal/mahasiswa" ],
		"js": [ "loader.js" ]
	}],

	"web_accessible_resources": ["absen_utils.js"],

	"permissions": [
		"activeTab"
	]
}